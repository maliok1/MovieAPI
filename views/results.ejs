<%- include ('partials/header')%>
<div class="results-area">
  <h1>Results Page</h1>
  <ul>
  <% data['Search'].forEach(function(movie){ %>
      <li>
      <strong><%= movie.Title %> </strong>- <%= movie.Year %>
      </li>
      <img src=<%=movie.Poster%> alt="">
    <% }) %>
  </ul>

  <a href="/">Search Again!</a>
</div>
<%- include ('partials/footer')%>